apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: storage-manager-mysql
  labels:
    caicloud-app: storage-manager-mysql
    caicloud.io/cluster-service: "true"
spec:
  storageClassName: heketi-storageclass
  accessModes:
    - ReadWriteMany
  resources:
    requests:
      storage: 1024Mi
