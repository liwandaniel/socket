apiVersion: v1
kind: Config
current-context: default
clusters:
- cluster:
    certificate-authority-data: authorityData
    server: "https://endpointIP:endpointPort"
  name: clusterName
contexts:
- context:
    cluster: clusterName
    user: kubectl
  name: default
users:
- name: kubectl
  user:
    client-certificate-data: certificateData
    client-key-data: keyData